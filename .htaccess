<IfModule mod_rewrite.c>
    RewriteEngine On
    RewriteBase /

    # Tambahkan kondisi untuk memastikan permintaan tidak sudah diarahkan ke direktori frontend
    RewriteCond %{REQUEST_URI} !^/frontend/

    # Redirect semua permintaan ke direktori public
    RewriteRule ^(.*)$ frontend/$1 [L]
</IfModule>
